<Window x:Class="WalletClientApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WalletClientApplication"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Closing="W_Closing">
    <Grid>
        <ListBox x:Name="board" HorizontalAlignment="Left" Height="268" Margin="60,85,0,0" VerticalAlignment="Top" Width="203"/>
        <TextBox x:Name="pythonInput" HorizontalAlignment="Left" Height="204" Margin="345,55,0,0" TextWrapping="Wrap" Text="Input Python Here" VerticalAlignment="Top" Width="370"/>
        <Button x:Name="button_submitPython" Content="Submit Python" HorizontalAlignment="Left" Margin="383,281,0,0" VerticalAlignment="Top" Width="275" Click="button_submitPython_Click" Height="72"/>
        <Label x:Name="errorOutput" HorizontalAlignment="Left" Margin="291,313,0,0" VerticalAlignment="Top" Content=" "/>
    </Grid>
</Window>
